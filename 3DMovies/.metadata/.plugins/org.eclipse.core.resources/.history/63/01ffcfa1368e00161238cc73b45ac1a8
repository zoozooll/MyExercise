package com.oregonscientific.meep.opengl;


public class MediaItem {
	
	public MediaItem()
	{
		mImageAddr = "";
		mAngle = 0;
		mName = "";
		mDesc = "";
		mPath = "";
		mTransform = null;
	}
	
	public String getImageAddr() {
		return mImageAddr;
	}

	public void setImageAddr(String mImageAddr) {
		this.mImageAddr = mImageAddr;
	}

	public float getAngle() {
		return mAngle;
	}
	public void setAngle(float angle) {
		this.mAngle = angle;
	}
	public String getName() {
		return mName;
	}
	public void setName(String name) {
		this.mName = name;
	}
	public String getDesc() {
		return mDesc;
	}
	public void setDesc(String desc) {
		this.mDesc = desc;
	}
	public String getPath() {
		return mPath;
	}
	public void setPath(String path) {
		this.mPath = path;
	}
	public ESTransform getTransform() {
		return mTransform;
	}
	public void setTransform(ESTransform transform) {
		this.mTransform = transform;
	}

	private String mImageAddr;
	private float mAngle;
	private String mName;
	private String mDesc;
	private String mPath;
	private ESTransform mTransform;
}
