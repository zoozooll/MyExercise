<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:keyWidth="@dimen/key_width"
    android:keyHeight="@dimen/key_height"    
    android:horizontalGap="2px"
    android:verticalGap="2px"
    >
    <Row></Row>
	<Row></Row>
	<Row></Row>
    <Row>
        <Key android:codes="-5"  android:keyWidth="42px" android:keyIcon="@drawable/blackkey_del" android:iconPreview="@drawable/icon_del"
        android:isRepeatable="true" android:keyEdgeFlags="left"/>
        <Key android:codes="49" android:keyLabel="1"/>
        <Key android:codes="50" android:keyLabel="2"/>
        <Key android:codes="51" android:keyLabel="3"/>
        <Key android:codes="52" android:keyLabel="4"/>
        <Key android:codes="53" android:keyLabel="5"/>
        <Key android:codes="54" android:keyLabel="6" android:horizontalGap = "275px"/>
        <Key android:codes="55" android:keyLabel="7"/>
        <Key android:codes="56" android:keyLabel="8"/>
        <Key android:codes="57" android:keyLabel="9"/>
        <Key android:codes="48" android:keyLabel="0"/>
        <Key android:codes="-5"  android:keyWidth="42px" android:keyIcon="@drawable/blackkey_del" android:iconPreview="@drawable/icon_del"
        android:isRepeatable="true" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
		<Key android:codes="0"  android:keyWidth="55px" android:keyIcon="@drawable/blackkey_photo" android:iconPreview="@drawable/icon_photo"
                android:keyEdgeFlags="left"/>
        <Key android:codes="64" android:keyLabel="\@"/>
        <Key android:codes="35" android:keyLabel="\#"/>
        <Key android:codes="36" android:keyLabel="$"/>
        <Key android:codes="37" android:keyLabel="%"/>
        <Key android:codes="38" android:keyLabel="&amp;"/>
        <Key android:codes="42" android:keyLabel="*" android:horizontalGap = "250px"/>
        <Key android:codes="45" android:keyLabel="-"/>
        <Key android:codes="61" android:keyLabel="="/>
        <Key android:codes="40" android:keyLabel="("/>
        <Key android:codes="41" android:keyLabel=")"/>
        <Key android:codes="0" android:keyWidth="55px" android:keyIcon ="@drawable/blackkey_video"  android:iconPreview="@drawable/icon_video"
            android:keyEdgeFlags="right"/>
    </Row>
    
    <Row>
        <Key android:codes="-1"  android:keyWidth="65px" android:keyIcon="@drawable/blackkey_shift" android:iconPreview="@drawable/icon_shift"
                android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="33" android:keyLabel="!" />
        <Key android:codes="34" android:keyLabel="&quot;"/>
        <Key android:codes="39" android:keyLabel="\'"/>
        <Key android:codes="58" android:keyLabel=":"/>
        <Key android:codes="59" android:keyLabel=";"/>
        <Key android:codes="44" android:keyLabel="," android:horizontalGap = "228px"/>
        <Key android:codes="43" android:keyLabel="+"/>
        <Key android:codes="91" android:keyLabel="["/>
        <Key android:codes="93" android:keyLabel="]"/>
        <Key android:codes="63" android:keyLabel="\?"/>
        <Key android:codes="-1"  android:keyWidth="65px" android:keyIcon="@drawable/blackkey_shift" android:iconPreview="@drawable/icon_shift"
                android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="right"/>
    </Row>
    
    <Row  android:rowEdgeFlags="bottom">
        <Key android:codes="-3"   android:keyWidth="65px" android:keyIcon="@drawable/blackkey_input" android:iconPreview="@drawable/icon_input"
             android:keyEdgeFlags="left"/>
        <Key android:codes="-2" android:keyIcon="@drawable/blackkey_abc" android:iconPreview="@drawable/icon_abc" />
        <Key android:codes="0" android:keyIcon="@drawable/blackkey_icon" android:iconPreview="@drawable/icon_icon" 
            android:popupKeyboard="@xml/popup_smileys" />    
        <Key android:codes="47" android:keyIcon="@drawable/blackkey_slith" android:iconPreview="@drawable/icon_slith" />
        <Key android:codes="32" android:keyIcon="@drawable/whitekey_space" android:iconPreview="@drawable/icon_space" 
             android:keyWidth="84px" android:isRepeatable="true"/>
        <Key android:codes="32" android:keyIcon="@drawable/whitekey_space" android:iconPreview="@drawable/icon_space" 
             android:keyWidth="84px" android:isRepeatable="true" android:horizontalGap = "230px"/>
        <Key android:codes="999" android:keyIcon="@drawable/blackkey_com"  android:horizontalGap = "4px" android:iconPreview="@drawable/icon_com" />
        <Key android:codes="0" android:keyIcon="@drawable/whitekey_left" android:iconPreview="@drawable/icon_left" />
        <Key android:codes="0" android:keyIcon="@drawable/blackkey_right" android:iconPreview="@drawable/icon_right" />
        <Key android:codes="10" android:keyIcon="@drawable/blackkey_send" android:iconPreview="@drawable/icon_send"
             android:keyEdgeFlags="right" android:horizontalGap = "13px"/>
    </Row>
</Keyboard>